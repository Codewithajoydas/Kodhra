    <script>
      const dropBox = document.querySelector(".upload-box");
      const filename = document.getElementById("filename");
      const uploadBtn = document.getElementById("upload");
      const cancelBtn = document.getElementById("cancel");
      const manualInput = document.getElementById("file");
      let currentFile = null;
      function handleFile(file) {
        const ext = file.name.split(".").pop().toLowerCase();
        if (ext !== "json") {
          new Toastmaster({
            title: "Invalid File",
            message: "Please upload a valid JSON file",
            type: "error",
            delay: 5000
          }).showNotification();
          return;
        }

        currentFile = file;

        const reader = new FileReader();
        reader.onload = () => {
          filename.innerHTML = `
        <span style="font-weight: 400; display:flex;justify-content: center;gap:5px;align-items: center;">
          <svg width="20" height="20" ...></svg>
          <span style="width: 300px;text-align: center;display: -webkit-box;-webkit-line-clamp: 1;-webkit-box-orient: vertical;overflow: hidden;text-overflow: ellipsis;">
            ${file.name}${file.size / 1024 / 1024 > 1
              ? ` (${(file.size / 1024 / 1024).toFixed(2)} MB)`
              : ` (${(file.size / 1024).toFixed(2)} KB)`}
          </span>
        </span>
      `;
        };
        reader.readAsText(file);
      }

      dropBox.addEventListener("dragover", (e) => {
        e.preventDefault();
        dropBox.classList.add("drag-active");
      });

      dropBox.addEventListener("dragleave", () => {
        dropBox.classList.remove("drag-active");
      });

      dropBox.addEventListener("drop", (e) => {
        e.preventDefault();
        dropBox.classList.remove("drag-active");
        const files = e.dataTransfer.files;
        if (files.length > 0) {
          handleFile(files[0]);
        }
      });



      dropBox.addEventListener("change", (e) => {
        const file = e.target.files[0];
        if (file) handleFile(file);
      });

      uploadBtn.addEventListener("click", async () => {
        if (!currentFile) return;

        const formData = new FormData();
        formData.append("file", currentFile);

        const res = await fetch("/import-export", {
          method: "POST",
          body: formData,
          credentials: "include"
        });

        const data = await res.json();

        if (res.ok) {
          new Toastmaster({
            title: "Success",
            message: "Imported Successfully",
            type: "success",
            delay: 5000
          }).showNotification();
        } else {
          new Toastmaster({
            title: "Error",
            message: "Something went wrong",
            type: "error",
            delay: 5000
          }).showNotification();
        }

        currentFile = null;
        filename.innerHTML = "Drag & Drop";
        manualInput.value = "";
      });

      cancelBtn.addEventListener("click", () => {
        currentFile = null;
        filename.innerHTML = "Drag & Drop";
        manualInput.value = "";
      });

    </script>

    <script>
      const searchText = document.getElementById("asd");
      const language = document.getElementById("asa");
      const startDate = document.getElementById("asb");
      const endDate = document.getElementById("asc");
      document.getElementById("export").addEventListener("click", async () => {
        if (startDate.value > endDate.value) {
          new Toastmaster({
            title: "Error",
            message: "Start Date cannot be greater than End Date",
            type: "error",
            delay: 5000
          }).showNotification();
          return
        }
        if (!startDate.value && !endDate.value) {
          new Toastmaster({
            title: "Error",
            message: "Please fill all the fields",
            type: "error",
            delay: 5000
          }).showNotification();
          return
        }
        document.getElementById("export").disabled = true;
        document.getElementById("export").textContent = "Downloading...";
        const res = await fetch("/card/download", {
          method: "POST",
          headers: { "Content-Type": "application/json" },
          body: JSON.stringify({
            query: searchText.value,
            language: language.value,
            startDate: startDate.value,
            endDate: endDate.value
          }),
          credentials: "include"
        })
        const data = await res.json();
        if (data.cards.length === 0) {
          new Toastmaster({
            title: "Add Query",
            message: "Please add a query",
            type: "error",
            delay: 5000
          }).showNotification();
          return
        }
        const blob = new Blob([JSON.stringify(data, null, 2)], {
          type: "application/json",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "cards.json";
        a.click();
        URL.revokeObjectURL(url);
        document.getElementById("export").disabled = false;
        document.getElementById("export").textContent = "Export";
      })


    </script>

