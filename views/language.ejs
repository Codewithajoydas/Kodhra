<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Languages | Code Snippet Manager</title>
    <link rel="stylesheet" href="/style/css/partials/Header.css" />
    <link rel="stylesheet" href="/style/css/ad_search.css" />
    <link rel="stylesheet" href="/style/css/Notification.css" />
    <link rel="stylesheet" href="/style/css/index.css" />
    <link rel="stylesheet" href="/style/css/chip.css" />
    <link rel="stylesheet" href="/style/css/card.css" />
    <link rel="stylesheet" href="/style/css/loader.css" />
    <link rel="stylesheet" href="/style/css/folder.css" />
    <link rel="stylesheet" href="/style/css/toast.css" />
        <link rel="shortcut icon" href="favicon.ico" type="image/x-icon" />
  <link rel="shortcut icon" href="/assets/logo.webp" type="image/png" />
<script src="\scripts\shortcut.js"></script>

    <script src="/socket.io/socket.io.js"></script>

    <link rel="shortcut icon" href="/assets/logo.png" type="image/png" />
    <script src="https://unpkg.com/colorthief/dist/color-thief.umd.js"></script>
    <script src="https://unpkg.com/@lottiefiles/dotlottie-wc@0.6.2/dist/dotlottie-wc.js" type="module"></script>
    <link rel="preload" href="/gEzG0w1WVY.lottie" as="fetch" crossorigin="anonymous" />
    
    <style>
        .lan_cards {
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
        }

        .languageCard {
            margin: 20px;
            width: 200px;
            height: 200px;
            border-radius: 10px;
            background: var(--bg-color);
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            gap: 10px;
            transition: 0.3s;
            box-shadow: var(--box-shadow) !important;
        }

        .languageCard:hover {
            cursor: pointer;
            transform: translateY(-10px) scale(1.01);
        }

        .languageName {
            font-weight: 600;
            font-size: 20px;
        }
    </style>

</head>

<body data-theme="light">
 <%-include("partials/loader.ejs") %>
    <div class="content">
        <%- include("partials/header.ejs")%>
            <div class="container">
                <%- include("partials/sidebar.ejs", { navigation: 'lan' })%>
                    <main style="height: calc(100vh - 70px)">
                        <% if(cards.length !== 0){%>
                            <div class="page_title" onclick="collapse(this)">
                                <span class="down"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none"
                                        stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                                        class="lucide lucide-chevron-down-icon lucide-chevron-down">
                                        <path d="m6 9 6 6 6-6" />
                                    </svg></span>
                                Language Section
                            </div>
                            <div class="lan_cards">
                                <% cards.forEach(language=> {%>
                                    <div class="languageCard"
                                        onclick="window.location.assign('/search/json?q=<%= language._id %>')">
                                        <div class="banner">
                                            <img height="100" width="100" loading="lazy"
                                                src="https://cdn.simpleicons.org/<%= 
                                                                                                                language._id === 'JavaScript' ? 'javascript' :
      language._id === 'CSS' ? 'css' :
      language._id === 'HTML' ? 'html5' :
      language._id === 'C' ? 'c' :
      language._id === 'C++' ? 'cplusplus' :
      language._id === 'C#' ? 'csharp' :
      language._id === 'Java' ? 'java' :
      language._id === 'TypeScript' ? 'typescript' :
      language._id === 'Python' ? 'python' :
      language._id === 'Ruby' ? 'ruby' :
      language._id === 'PHP' ? 'php' :
      language._id === 'Go' ? 'go' :
      language._id === 'Rust' ? 'rust' :
      language._id === 'Swift' ? 'swift' :
      language._id === 'Kotlin' ? 'kotlin' :
      language._id === 'Objective-C' ? 'objectivec' :
      language._id === 'Scala' ? 'scala' :
      language._id === 'Perl' ? 'perl' :
      language._id === 'R' ? 'r' :
      language._id === 'Dart' ? 'dart' :
      language._id === 'Haskell' ? 'haskell' :
      language._id === 'Lua' ? 'lua' :
      language._id === 'Elixir' ? 'elixir' :
      language._id === 'Clojure' ? 'clojure' :
      language._id === 'Shell' ? 'gnubash' :
      language._id === 'PowerShell' ? 'powershell' :
      language._id === 'SQL' ? 'mysql' :
      language._id === 'GraphQL' ? 'graphql' :
      language._id === 'MATLAB' ? 'mathworks' :
      language._id === 'Assembly' ? 'assemblyscript' :
      language._id === 'Fortran' ? 'fortran' :
      language._id === 'COBOL' ? 'cobol' :
      language._id === 'Erlang' ? 'erlang' :
      language._id === 'VB.NET' ? 'vbnet' :
                                                                                                                language._id.toLowerCase().replace(/[+#.\s()]/g, '')%>"
                                                alt="<%= language %> icon"
                                                onerror="this.src = 'https://cdn-icons-png.flaticon.com/512/1049/1049443.png'" />
                                        </div>
                                        <p class="languageName">
                                            <%= language._id %>
                                        </p>
                                        <span style="font-size: smaller !important;display: inline-block !important;">
                                            <%= language.count %>
                                                Snippets
                                        </span>
                                    </div>
                                    <% }) %>
                            </div>
                        <%} else{%>
                            <div class="nodata" style="display: flex; justify-content: center; align-items: center;height: calc(100vh - 100px);">
                                <%- include("partials/no-data.ejs")%>
                            </div>
                        <%}%>
                        <%- include("partials/fab.ejs")%>
                    </main>

                    <div id="context_menu" style="
            position: absolute;
            top: 0;
            left: 0;
            display: none;
            z-index: 1000000;
            width: 200px;
          ">
                        <ul id="context_menu_list"></ul>
                    </div>
                    <div class="networkInfo" style="display: none">
                        <span><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"
                                fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"
                                stroke-linejoin="round" class="lucide lucide-info-icon lucide-info">
                                <circle cx="12" cy="12" r="10" />
                                <path d="M12 16v-4" />
                                <path d="M12 8h.01" />
                            </svg></span>

                        <span id="networkInfo"></span>
                    </div>
            </div>
    </div>



    <script src="/scripts/ad_search.js"></script>
    <script src="/scripts/Fab.js"></script>
    <script src="/scripts/Notification.js"></script>
    <script src="/scripts/searchFunction.js"></script>
    <script src="/scripts/context_menu.js"></script>
    <script src="\scripts\isonline.js"></script>
    <script src="/scripts/header.js"></script>
    <script src="/scripts/collapse.js"></script>
</body>

</html>